{
    "output": "DP-3",
    "modules-right": [
        "custom/mute-status",
        "pulseaudio",
        "cpu",
        "memory",
        "temperature",
        "tray",
        "clock#time"
    ],

  "custom/mute-status": {
    "format": "{text}",
    "exec":  "bash ~/.config/waybar/scripts/mute-status.sh",
    "interval": 2,
    "on-click": "bash -c 'pactl set-source-mute @DEFAULT_SOURCE@ toggle'"
  }
}
